{
  "name": "iamcalling-app",
  "version": "1.0.0",
  "description": "IAmCalling Application with ideological analysis and messaging",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node module-test-runner.js",
    "test:messenger": "mocha \"tests/unit/messenger/**/*.test.js\"",
    "test:e2e": "node run-e2e-tests.js",
    "test:quick": "node quick-e2e-test.js",
    "test:cypress": "cypress run --spec tests/e2e/comprehensive-platform-test.cy.js",
    "test:cypress:open": "cypress open",
    "test:all": "npm run test:quick && npm run test:e2e",
    "build": "echo 'No build step required'",
    "postinstall": "echo 'Installation complete'"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.93.3",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.3",
    "compression": "^1.7.4",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "ejs": "^4.0.1",
    "express": "^5.2.1",
    "express-rate-limit": "^7.2.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "web-push": "^3.6.7",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "chai": "^6.2.2",
    "cypress": "^15.9.0",
    "mocha": "^11.7.5",
    "supertest": "^7.2.2"
  }
}
